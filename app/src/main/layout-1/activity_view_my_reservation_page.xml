<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".search_parking_spot_by_parkinguser">



    <TextView
        android:id="@+id/textView"
        android:layout_width="match_parent"
        android:layout_height="53dp"
        android:text="VIEW MY RESERVATIONS"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:gravity="center_horizontal"
        android:layout_marginTop="0dp"
        android:textColor="#000000"
        android:textSize="30sp"/>


    <TextView
        android:id="@+id/filter1"
        android:layout_width="130dp"
        android:layout_height="44dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="54dp"
        android:layout_marginRight="276dp"
        android:text="Start Time :"
        android:textColor="#000000"
        android:textSize="12sp" />

    <SearchView
        android:id="@+id/search"
        android:layout_width="130dp"
        android:layout_height="44dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_marginLeft="102dp"
        android:layout_marginTop="41dp"
        android:layout_marginRight="120dp"
        android:queryHint="Start Time" />

    <TextView
        android:id="@+id/filter2"
        android:layout_width="130dp"
        android:layout_height="44dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="102dp"
        android:layout_marginRight="276dp"
        android:text="Date :"
        android:textColor="#000000"
        android:textSize="12sp" />


    <SearchView
        android:id="@+id/search2"
        android:layout_width="130dp"
        android:layout_height="44dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_marginLeft="103dp"
        android:layout_marginTop="92dp"
        android:layout_marginRight="119dp"
        android:queryHint="Date" />




    <TextView
        android:id="@+id/textView1"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="200dp"
        android:textColor="#000000"
        android:ems="10"
        android:textSize="20sp"
        android:text="Start Time: 7:30PM"
        android:inputType="time" />


    <TextView
        android:id="@+id/textView2"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="220dp"
        android:textColor="#000000"
        android:ems="10"
        android:textSize="20sp"
        android:text="Duration of Parking: 3 HOUR"
        android:inputType="time" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="240dp"
        android:textColor="#000000"
        android:ems="10"
        android:textSize="20sp"
        android:text="Parking Area Name:MAC"
        android:inputType="textPersonName" />
    <TextView
        android:id="@+id/textView5"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="260dp"
        android:textColor="#000000"
        android:ems="10"
        android:textSize="20sp"
        android:text="Floor number:3"
        android:inputType="number" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="280dp"
        android:textColor="#000000"
        android:ems="10"
        android:textSize="20sp"
        android:text="Parking Type:Basic"
        android:inputType="textPersonName" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="300dp"
        android:textColor="#000000"
        android:ems="10"
        android:textSize="20sp"
        android:text="Parking Spot Number:10"
        android:inputType="number" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="320dp"
        android:textColor="#000000"
        android:ems="10"
        android:textSize="20sp"
        android:text="Reservation No:15"
        android:inputType="number" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="340dp"
        android:textColor="#000000"
        android:ems="10"
        android:textSize="20sp"
        android:text="Parking option selected:History"
        android:inputType="number" />


    <Button
        android:id="@+id/SELECT1"
        android:layout_width="100dp"
        android:layout_height="60dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="300dp"
        android:layout_marginTop="260dp"
        android:textSize="15sp"
        android:background="#78909C"
        android:text="SELECT" />





</RelativeLayout>